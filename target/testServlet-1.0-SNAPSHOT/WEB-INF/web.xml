<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>MyServlet</servlet-name>
        <servlet-class>MyServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Start</servlet-name>
        <servlet-class>nz.ac.massey.cs.webtech.s19041253.Start</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Hit</servlet-name>
        <servlet-class>nz.ac.massey.cs.webtech.s19041253.Hit</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Stand</servlet-name>
        <servlet-class>nz.ac.massey.cs.webtech.s19041253.Stand</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Won</servlet-name>
        <servlet-class>nz.ac.massey.cs.webtech.s19041253.Won</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Stats</servlet-name>
        <servlet-class>nz.ac.massey.cs.webtech.s19041253.Stats</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Start</servlet-name>
        <url-pattern>/jack/start</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Hit</servlet-name>
        <url-pattern>/jack/move/hit</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Stand</servlet-name>
        <url-pattern>/jack/move/stand</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Won</servlet-name>
        <url-pattern>/jack/won</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Stats</servlet-name>
        <url-pattern>/jack/stats</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MyServlet</servlet-name>
        <url-pattern>/MyServlet</url-pattern>
    </servlet-mapping>

</web-app>